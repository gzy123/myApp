{
  "name": "my-app",
  "version": "1.0.0",
  "private": true,
  "description": "my-app",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:html": "htmlhint **/*.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "FORCE_COLOR=1 node script/verify-commit.ts",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "test/**/*.{js,jsx,ts,tsx}": "eslint",
    "*.css": "stylelint",
    "*.html": "htmlhint"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "owenGuo",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.3.14",
    "@tarojs/components": "3.3.14",
    "@tarojs/runtime": "3.3.14",
    "@tarojs/taro": "3.3.14",
    "lodash": "4.17.15",
    "@tarojs/react": "3.3.14",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "taro-ui": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.3.14",
    "@tarojs/webpack-runner": "3.3.14",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.3.14",
    "chalk": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.3.14",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "^26.0.0",
    "typescript": "^4.1.0"
  }
}
